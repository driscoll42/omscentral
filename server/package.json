{
  "private": true,
  "name": "omscentral-server",
  "version": "0.0.0",
  "description": "",
  "license": "ISC",
  "author": "Mehmet Bajin <mehmet.bajin@gatech.edu>",
  "main": "dist/src/server.js",
  "scripts": {
    "build": "npm run tsc",
    "dev": "npm run watch:api",
    "format": "prettier --write '(src|database)/{**/*,*}.{js,ts,json}' && prettier --write '*.{js,json}' && prettier --write --parser json '.prettierrc'",
    "format:package": "prettier-package-json --write ./package.json",
    "knex": "knex --knexfile database/knexfile.ts",
    "start": "node --optimize_for_size --max_old_space_size=460 --gc_interval=100 dist/src/server.js",
    "test": "run-s test:formatting test:compile test:lint",
    "test:compile": "npm run tsc -- --noEmit",
    "test:formatting": "prettier --check '(src|database)/{**/*,*}.{js,ts,json}' && prettier --check '*.{js,json}' && prettier --check --parser json '.prettierrc'",
    "test:lint": "eslint src/ database/ --ext .js,.ts",
    "ts-node": "ts-node",
    "tsc": "tsc -p tsconfig.json",
    "watch:api": "ts-node-dev --no-notify --respawn --transpile-only src/server.ts"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.13.0",
    "@hapi/boom": "9.1.2",
    "@hapi/joi": "17.1.1",
    "@sentry/node": "6.5.0",
    "apollo-server-express": "2.25.0",
    "bluebird": "3.7.2",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "10.0.0",
    "dotenv-expand": "5.1.0",
    "express": "4.17.1",
    "express-rate-limit": "5.2.6",
    "firebase-admin": "9.9.0",
    "graphql": "15.5.0",
    "graphql-middleware": "6.0.10",
    "graphql-shield": "7.5.0",
    "graphql-tag": "2.12.4",
    "graphql-tools": "7.0.5",
    "html-minifier": "4.0.0",
    "knex": "0.21.19",
    "lodash": "4.17.21",
    "lusca": "1.7.0",
    "moment": "2.29.1",
    "morgan": "1.10.0",
    "objection": "2.2.15",
    "objection-db-errors": "1.1.2",
    "objection-visibility": "1.1.0",
    "pg": "8.6.0",
    "request": "2.88.2",
    "request-promise": "4.2.6",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.35",
    "@types/body-parser": "1.19.0",
    "@types/boom": "7.3.0",
    "@types/compression": "1.7.0",
    "@types/cors": "2.8.10",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.12",
    "@types/express-rate-limit": "5.1.1",
    "@types/graphql": "14.5.0",
    "@types/hapi__joi": "17.1.6",
    "@types/html-minifier": "4.0.0",
    "@types/jest": "26.0.23",
    "@types/lodash": "4.14.170",
    "@types/lusca": "1.7.0",
    "@types/morgan": "1.9.2",
    "@types/node": "15.6.1",
    "@types/pg": "8.6.0",
    "@types/request": "2.48.5",
    "@types/request-promise": "4.1.47",
    "@types/winston": "2.4.4",
    "@types/yargs": "17.0.0",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "chokidar-cli": "2.1.0",
    "concurrently": "6.2.0",
    "cross-env": "7.0.3",
    "eslint": "7.27.0",
    "eslint-plugin-flowtype": "5.7.2",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "graphql-request": "3.4.0",
    "jest": "27.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.0",
    "prettier-package-json": "2.6.0",
    "ts-jest": "27.0.2",
    "ts-node": "10.0.0",
    "ts-node-dev": "latest",
    "tslint": "6.1.3",
