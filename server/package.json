{
  "private": true,
  "name": "omscentral-server",
  "description": "",
  "license": "ISC",
  "author": "Mehmet Bajin <mehmet.bajin@gatech.edu>",
  "version": "0.0.0",
  "main": "dist/src/server.js",
  "scripts": {
    "build": "npm run tsc",
    "dev": "npm run watch:api",
    "format": "prettier --write '(src|database)/{**/*,*}.{js,ts,json}'",
    "format:package": "prettier-package-json --write ./package.json",
    "knex": "knex --knexfile database/knexfile.ts",
    "start": "node --optimize_for_size --max_old_space_size=460 --gc_interval=100 dist/src/server.js",
    "test": "run-s test:formatting test:compile test:lint",
    "test:compile": "npm run tsc -- --noEmit",
    "test:formatting": "prettier --check '(src|database)/{**/*,*}.{js,ts,json}'",
    "test:lint": "eslint src --ext .js,.ts",
    "ts-node": "ts-node",
    "tsc": "tsc -p tsconfig.json",
    "watch:api": "ts-node-dev --no-notify --respawn --transpile-only src/server.ts"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.8.0",
    "@hapi/boom": "9.1.0",
    "@hapi/joi": "17.1.1",
    "@sentry/node": "5.20.1",
    "bluebird": "3.7.2",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "express": "4.17.1",
    "express-graphql": "0.11.0",
    "express-rate-limit": "5.1.3",
    "firebase-admin": "9.0.0",
    "graphql": "15.3.0",
    "graphql-middleware": "4.0.2",
    "graphql-shield": "7.3.3",
    "graphql-tag": "2.11.0",
    "graphql-tools": "6.0.16",
    "html-minifier": "4.0.0",
    "knex": "0.21.3",
    "lodash": "4.17.19",
    "lusca": "1.6.1",
    "moment": "2.27.0",
    "morgan": "1.10.0",
    "objection": "2.2.2",
    "objection-db-errors": "1.1.2",
    "objection-graphql": "0.4.5",
    "objection-visibility": "1.1.0",
    "pg": "8.3.0",
    "request": "2.88.2",
    "request-promise": "4.2.6",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.32",
    "@types/body-parser": "1.19.0",
    "@types/boom": "7.3.0",
    "@types/compression": "1.7.0",
    "@types/cors": "2.8.7",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.7",
    "@types/express-rate-limit": "5.0.0",
    "@types/graphql": "14.5.0",
    "@types/hapi__joi": "17.1.4",
    "@types/html-minifier": "4.0.0",
    "@types/jest": "26.0.9",
    "@types/lodash": "4.14.159",
    "@types/lusca": "1.6.2",
    "@types/morgan": "1.9.1",
    "@types/node": "14.0.27",
    "@types/pg": "7.14.4",
    "@types/request": "2.48.5",
    "@types/request-promise": "4.1.46",
    "@types/winston": "2.4.4",
    "@types/yargs": "15.0.5",
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "chokidar-cli": "2.1.0",
    "concurrently": "5.3.0",
    "cross-env": "7.0.2",
    "eslint": "6.x",
    "eslint-plugin-graphql": "4.0.0",
    "graphql-request": "3.0.0",
    "jest": "26.2.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "prettier-package-json": "2.1.3",
    "ts-jest": "26.1.4",
    "ts-node": "8.10.2",
    "ts-node-dev": "latest",
    "tslint": "6.1.3",
    "typescript": "3.9.7",
    "yargs": "15.4.1"
  },
  "engines": {
    "node": ">=14"
  }
}
