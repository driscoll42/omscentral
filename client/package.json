{
  "private": true,
  "name": "omscentral-client",
  "description": "",
  "license": "ISC",
  "author": "Mehmet Bajin <mehmet.bajin@gatech.edu>",
  "version": "0.0.0",
  "main": "build/index.html",
  "scripts": {
    "build": "react-scripts build",
    "build:local": "env-cmd -f .env.local react-scripts build",
    "build:production": "env-cmd -f .env.production react-scripts build",
    "build:staging": "env-cmd -f .env.staging react-scripts build",
    "cypress": "cypress",
    "dev": "react-scripts start",
    "firebase": "firebase",
    "format": "prettier --write '(src|cypress)/{**/*,*}.{js,jsx,ts,tsx,json,css,scss}'",
    "format:package": "prettier-package-json --write ./package.json",
    "start": "serve -s build -p 3000",
    "test": "run-s test:formatting test:lint test:compile test:react",
    "test:compile": "tsc -p tsconfig.json",
    "test:formatting": "prettier --check '(src|cypress)/{**/*,*}.{js,jsx,ts,tsx,json,css,scss}'",
    "test:integration": "cypress run",
    "test:lint": "eslint src/ cypress/ --ext .ts,.tsx,.js,.jsx",
    "test:react": "react-scripts test"
  },
  "dependencies": {
    "@apollo/client": "3.2.7",
    "@apollo/react-hooks": "4.0.0",
    "@material-ui/core": "4.11.2",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.57",
    "@sentry/browser": "5.30.0",
    "clsx": "1.1.1",
    "firebase": "8.2.2",
    "graphql": "15.4.0",
    "graphql-tag": "2.11.0",
    "query-string": "6.13.8",
    "react": "17.0.1",
    "react-apollo": "3.1.5",
    "react-copy-to-clipboard": "5.0.3",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-hook-form": "6.14.1",
    "react-markdown": "5.0.3",
    "react-router-dom": "5.2.0",
    "react-visibility-sensor": "5.1.1",
    "subscriptions-transport-ws": "0.9.18"
  },
  "devDependencies": {
    "@apollo/react-testing": "4.0.0",
    "@types/firebase": "3.2.1",
    "@types/jest": "26.0.20",
    "@types/query-string": "6.3.0",
    "@types/react": "17.0.0",
    "@types/react-copy-to-clipboard": "5.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-helmet": "6.1.0",
    "@types/react-router-dom": "5.1.7",
    "@typescript-eslint/eslint-plugin": "4.13.0",
    "@typescript-eslint/parser": "4.13.0",
    "cypress": "6.2.1",
    "env-cmd": "10.1.0",
    "eslint": "7.17.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-security": "1.4.0",
    "fibers": "5.0.0",
    "firebase-tools": "9.2.0",
    "node-sass": "5.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "prettier-package-json": "2.1.3",
    "react-scripts": "4.0.1",
    "sass": "1.32.4",
    "serve": "11.3.2",
    "typescript": "4.1.3"
  },
  "engines": {
    "node": "14.x"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
