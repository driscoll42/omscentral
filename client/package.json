    "env-cmd": "10.1.0",
    "eslint": "6.x",
    "eslint-plugin-flowtype": "4.x",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-react": "7.20.3",
    "eslint-plugin-react-hooks": "2.x",
    "eslint-plugin-security": "1.4.0",
    "fibers": "5.0.0",
    "firebase-tools": "8.4.3",
    "node-sass": "4.14.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "prettier-package-json": "2.1.3",
    "react-scripts": "3.4.1",
    "sass": "1.26.9",
    "serve": "11.3.2",
    "typescript": "3.9.6"
  },
  "engines": {
